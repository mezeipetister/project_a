{{#*inline "page"}}

<section id="login">
  <h1>Hi {{name}}</h1>
  <p>Your age is {{age}}</p>
  <form action="/login" method="POST">
    Login <br>
    <input type="text" name="username" id="username" placeholder="Username"><br>
    <input type="password" name="password" id="password" placeholder="Password"><br>
    <input type="submit" value="Login">
  </form>
</section>

{{/inline}}
{{~> (parent)~}}